RewriteEngine On

RewriteRule ^login$ login.php [L]
RewriteRule ^lockscreen$ lockscreen.php [L]
RewriteRule ^logout$ login.php?act=logout [L]
RewriteRule ^BenangKeluarLegacy-$ index.php?p=BenangKeluarLegacy [L]
RewriteRule ^BenangKeluarLegacy-([0-9_-]+)$ index.php?p=BenangKeluarLegacy&doc=$1 [L]
RewriteRule ^CheckStockBS-$ index.php?p=CheckStockBS [L]
RewriteRule ^CheckStockBS-([a-zA-Z0-9_-]+)$ index.php?p=CheckStockBS&id=$1 [L]
RewriteRule ^DelStokKeluar-([0-9_-]+)$ index.php?p=DelStokKeluar&id=$1 [L]
RewriteRule ^DetailOpname-$ index.php?p=DetailOpname[L]
RewriteRule ^DetailOpname-([a-zA-Z0-9_-]+)-([a-zA-Z0-9_-]+)$ index.php?p=DetailOpname&tgl=$1&tipe=$2 [L]
RewriteRule ^DetailOpname11-$ index.php?p=DetailOpname11[L]
RewriteRule ^DetailOpname11-([a-zA-Z0-9_-]+)-([a-zA-Z0-9_-]+)$ index.php?p=DetailOpname11&tgl=$1&tipe=$2 [L]
RewriteRule ^DetailOpnameDetail11-$ index.php?p=DetailOpnameDetail11[L]
